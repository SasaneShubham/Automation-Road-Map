package Methods;

public class Static_Method_1 {

	
	public static void add() {
		
		int a=20;
		int b=33;
		
		int addition=a+b;
		System.out.println("addition :"+ addition);
		
	}
	
     public static void sub() {
		
		int a=20;
		int b=33;
		
		int subtraction=b-a;
		System.out.println("subtraction :" +subtraction);
		
	}
     public static void main(String[] args) {
		
    	 Static_Method_1.add();          //static method call from same class
    	 Static_Method_1.sub();
    	 
    	 
    	 Diff_Class.div();                //static method call from different class
	}
}
